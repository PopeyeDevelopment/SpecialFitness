<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CurvedImageView">
        <attr name="curvature" format="dimension" />
        <attr name="curvedTintAlpha" format="integer" />
        <attr name="curvedTintColor" format="color" />
        <attr name="curvedTintMode" format="integer">
            <flag name="automatic" value="0" />
            <flag name="manual" value="1"/>
        </attr>
        <attr name="gravity" format="integer">
            <flag name="top" value="0"/>
            <flag name="bottom" value="1"/>
        </attr>
        <attr name="curvatureDirection" format="integer">
            <flag name="outward" value="0"/>
            <flag name="inward" value="1"/>
        </attr>
        <attr name="gradientDirection" format="integer">
            <flag name="top_to_bottom" value="0" />
            <flag name="bottom_to_top" value="1" />
            <flag name="left_to_right" value="2" />
            <flag name="right_to_left" value="3" />
        </attr>
        <attr name="gradientStartColor" format="color"/>
        <attr name="gradientEndColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TextAttributes" >
        <attr name="titleDisabledTextColour" format="color" />
    </declare-styleable>

    <declare-styleable name="SettingsListPreferences" >
        <attr name="titleDisabledTextColour" />
        <attr name="overridingDefaultValue" format="string" />
    </declare-styleable>

    <declare-styleable name="SettingsMultiSelectListPreference" >
        <attr name="titleDisabledTextColour"/>
        <attr name="overridingDefaultValues" format="reference" />
        <attr name="EnumlistType" format="enum">
            <enum name="WEEKDAYS" value="0" />
        </attr>
    </declare-styleable>
</resources>
